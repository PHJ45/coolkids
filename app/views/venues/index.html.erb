<%= render 'map' %>

<h3><%= @venues.count %> cards in system</h3>

<% @venues.reverse_each do |venue| %> 
<%= link_to venue do %>
  <div class = "well">
    <%= venue.name %> <br>
    <%=venue.address%> <%=venue.cross_street%> <br>
    Added: <%=time_ago_in_words(venue.created_at)%> ago 
  </div> 
  <% end %> 
<% end %> 
</table>






